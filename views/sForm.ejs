<%-include('mHeader')%>
<div class="clearfix"></div>
	
  <div class="content-wrapper">
    <div class="container-fluid">
      <div>
        <%if(messages.success){%>
          <div class="p-3 waves-effect waves-light text-white text-center">
            <%=messages.success%>
          </div>
          <%}%>
      </div>
    <div class="row mt-3">
      <div class="col-lg-12">
         <div class="card">
           <div class="card-body">
           <div class="card-title">Student Data Form</div>
           <hr>
            <form action="/manager/sRegisterdata" enctype="multipart/form-data" method="post">
           <div class="form-group">
            <label for="input-1">Student Name</label>
            <input type="text" name="name" class="form-control" id="input-1" placeholder="Enter Your Name">
           </div>
           <div class="form-group">
            <label for="input-2">Student Email</label>
            <input type="email" name="email" class="form-control" id="input-2" placeholder="Enter Your Email Address">
           </div>
           <div class="form-group">
            <label for="input-3">Student Mobile Number</label>
            <input type="text" name="phone" class="form-control" id="input-3" placeholder="Enter Your Mobile Number">
           </div>
           <div class="form-group">
            <label for="input-4">Father Name</label>
            <input type="text" name="fathername" class="form-control" id="input-4" placeholder="Enter Your Father Name">
           </div>
           <div class="form-group">
            <label for="input-3">Father Mobile Number</label>
            <input type="text" name="fatherphone" class="form-control" id="input-3" placeholder="Enter Your Father Mobile Number">
           </div>
           <div class="form-group">
            <label for="input-5">Course</label>
            <select name="course" class="form-control" id="input-5" id="">
              <option value="">-- Select Course --</option>
              <%course.forEach((s)=>{%>
                <option value="<%=s.course%>"><%=s.course%></option>
              <%})%>
            </select>
           </div>
           <div class="form-group">
            <label for="input-5">Batch Time</label>
            <select name="batchtime" class="form-control" id="input-5" id="">
              <option value="">-- Select Batch Time --</option>
              <option value="8:00">8:00</option>
              <option value="9:00">9:00</option>
              <option value="10:00">10:00</option>
              <option value="11:00">11:00</option>
            </select>
           </div>
           <div class="form-group">
            <label for="input-3">Admission Date</label>
            <input type="date" name="admissiondate" class="form-control" id="input-3" placeholder="Enter Your Admission Date">
           </div>
           <div class="form-group">
            <label for="input-3">Fees</label>
            <input type="text" name="feeamount" class="form-control" id="input-3" placeholder="Enter Your Fee Amount">
           </div>
           <div class="form-group">
            <label for="input-3">Address</label>
            <input type="text" name="address" class="form-control" id="input-3" placeholder="Enter Your Address">
           </div>
           <div class="form-group">
            <label for="input-3">Enter Your Image</label>
            <input type="file" name="img" class="form-control" id="input-3" placeholder="Enter Your Image">
           </div>
           <div class="form-group py-2">
             <div class="icheck-material-white">
            <input type="checkbox" id="user-checkbox1" checked=""/>
            <label for="user-checkbox1">I Agree Terms & Conditions</label>
            </div>
           </div>
           <div class="form-group">
            <button type="submit" class="btn btn-light px-5"><i class="icon-lock"></i> Register</button>
          </div>
          </form>
         </div>
         </div>
      </div>

    </div><!--End Row-->

	<!--start overlay-->
		  <div class="overlay toggle-menu"></div>
		<!--end overlay-->

    </div>
    <!-- End container-fluid-->
    
   </div><!--End content-wrapper-->
   <!--Start Back To Top Button-->
    <a href="javaScript:void();" class="back-to-top"><i class="fa fa-angle-double-up"></i> </a>
    <!--End Back To Top Button-->
	
<%-include('mFooter')%>